void check_cant2(GtkWidget *widget, gpointer data)

{

	if(cant_jug2==0)

		cant_jug2=1;//humvshum activas

	else if(cant_jug2==1)

		cant_jug2=0;

}

void check_cant6(GtkWidget *widget, gpointer data)

{

	if(cant_jug6==0)

		cant_jug6=1;//humvshum activas

	else if(cant_jug6==1)

		cant_jug6=0;

}

